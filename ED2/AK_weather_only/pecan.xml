<?xml version="1.0"?>
<pecan>
  <outdir>/data/dbfiles/ak_weather_only</outdir>
  <database>
    <bety>
      <driver>PostgreSQL</driver>
      <user>bety</user>
      <password>bety</password>
      <host>postgres</host>
      <dbname>bety</dbname>
      <write>TRUE</write>
    </bety>
    <dbfiles>/data/dbfiles</dbfiles>
  </database>
  <pfts>
    <pft>
      <name>SetariaWT</name>
      <ed2_pft_number>1</ed2_pft_number>
    </pft>
  </pfts>

  <meta.analysis>
    <iter>3000</iter>
    <random.effects>TRUE</random.effects>
    <threshold>1.2</threshold>
    <update>TRUE</update>
  </meta.analysis>

  <ensemble>
    <size>1</size>
    <variable>NPP</variable>
    <samplingspace>
      <parameters>
        <method>lhc</method>
      </parameters>
  </samplingspace>
  </ensemble>

  <model>
    <type>ED2</type>
    <binary>/groups/dlebauer/ed2_results/global_inputs/ed2_2.2.0_singularity.sh</binary>
    <id>14</id>
    <edin>/pecan/models/ed/inst/ED2IN.r2.2.0</edin>
    <config.header>
      <radiation>
        <lai_min>0.01</lai_min>
      </radiation>
      <ed_misc>
        <output_month>12</output_month>
      </ed_misc>
    </config.header>
    <phenol.scheme>0</phenol.scheme>
    <ed2in_tags>
      <PFT_1ST_CHECK>2</PFT_1ST_CHECK>
      <IED_INIT_MODE>0</IED_INIT_MODE>
    </ed2in_tags>
  </model>

  <run>
    <site>
      <id>1000004897</id>
      <met.start>2019-01-01 00:00:00</met.start>
      <met.end>2020-12-31 23:59:59</met.end>
    </site>

    <inputs>
      <met>
        <source>MERRA</source>
        <output>ED2</output>
      </met>
    </inputs>

    <start.date>2019/01/01</start.date>
    <end.date>2020/12/31</end.date>
  </run>
  
  <host>
    <name>localhost</name>
  </host>

</pecan>